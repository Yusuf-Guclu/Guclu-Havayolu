@{
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}
<link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-QWTKZyjpPEjISv5WaRU9OFeRpok6YctnYmDr5pNlyT2bRjXh0JMhjY6hW+ALEwIH" crossorigin="anonymous">
<script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/js/bootstrap.bundle.min.js" integrity="sha384-YvpcrYf0tY3lHB60NNkmXc5s9fDVZLESaAA55NDzOxhy9GkcIdslK1eN7N6jIeHz" crossorigin="anonymous"></script>

<div class="container">
    @model IEnumerable<Havayolu.Models.Domain.Kullanici>
    <div class="row">
        <div class="col-12">
            <table class="table table-hover">
                <thead class="table-secondary">
                    <tr>
                        <th>Adı</th>
                        <th>Soyadı</th>
                        <th>E-Posta</th>
                        <th>Telefon</th>
                        <th>Şifre</th>
                        <th>İşlem</th>
                    </tr>
                </thead>
                @foreach (var Kullanici in Model)
                {
                    <tbody>
                        <tr>
                            <td>@Kullanici.adi</td>
                            <td>@Kullanici.soyadi</td>
                            <td>@Kullanici.e_posta</td>
                            <td>@Kullanici.telefon</td>
                            <td>@Kullanici.sifre</td>
                            <td><a class="btn btn-danger" href="/Home/kullaniciKayitSil?Id=@Kullanici.Id">Sil</a></td>
                        </tr>
                    </tbody>
                }
            </table>
        </div>
    </div>
</div>